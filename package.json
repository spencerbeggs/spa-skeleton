{
	"name": "spa-skeleton",
	"version": "0.0.0",
	"description": "An opinionated front-to-back stack to bootstrap single-page-app projects. Uses Express, MongoDB, Backbone, Browserify, LESS and Gulp.",
	"repository": "https://github.com/spencerbeggs/spa-skeleton",
	"main": "index.js",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "Private",
	"scripts": {
		"start": "node index.js",
		"proxy": "sudo pfctl -evf hooks/pd.conf",
		"reinstall": "rm -rf node_modules;npm install",
		"dev": "NODE_ENV=development ./node_modules/gulp/bin/gulp.js dev",
		"build": "gulp build"
	},
	"dependencies": {
		"autoprefixer-core": "^5.1.7",
		"body-parser": "^1.10.1",
		"browser-sync": "2.2.3",
		"compression": "^1.3.0",
		"cookie-parser": "^1.3.3",
		"dynamic-reverse-proxy": "^0.6.0",
		"express": "^4.11.0",
		"express-device": "^0.3.11",
		"express-handlebars": "^1.1.0",
		"gulp-postcss": "^4.0.3",
		"html-minifier": "^0.7.0",
		"lodash": "^3.5.0",
		"serve-favicon": "^2.2.0"
	},
	"browser": {
		"config": "./config/index.js"
	},
	"engines": {
		"node": "0.12.x",
		"npm": "2.x"
	},
	"browserify": {
		"transform": [
			"envify"
		]
	},
	"devDependencies": {
		"browserify": "^9.0.3",
		"browserify-shim": "3.8.3",
		"envify": "^3.3.0",
		"gulp": "git://github.com/gulpjs/gulp.git#4.0",
		"gulp-autoprefixer": "^2.1.0",
		"gulp-beautify": "^1.1.0",
		"gulp-concat": "^2.4.1",
		"gulp-eol": "^0.1.1",
		"gulp-flatten": "0.0.4",
		"gulp-jsbeautifier": "0.0.6",
		"gulp-jshint": "^1.8.6",
		"gulp-json-format": "^1.0.0",
		"gulp-less": "^3.0.1",
		"gulp-minify-css": "^1.0.0",
		"gulp-nodemon": "^1.0.4",
		"gulp-notify": "^2.0.0",
		"gulp-plumber": "^1.0.0",
		"gulp-rename": "^1.2.0",
		"gulp-sourcemaps": "^1.5.0",
		"gulp-strip-debug": "^1.0.1",
		"gulp-uglify": "^1.0.1",
		"gulp-util": "^3.0.1",
		"hbsfy": "^2.2.0",
		"html": "0.0.7",
		"jquery": "^2.1.3",
		"less-plugin-autoprefix": "^1.3.0",
		"pretty-hrtime": "^1.0.0",
		"supervisor": "~0.6.0",
		"vinyl-buffer": "^1.0.0",
		"vinyl-source-stream": "^1.0.0",
		"watchify": "^2.2.1"
	},
	"subdomain": "suncastio",
	"domains": [
		"www.suncast.io",
		"suncast.io"
	]
}
