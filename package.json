{
	"name": "spa-skeleton",
	"version": "0.0.0",
	"description": "An opinionated front-to-back stack to bootstrap single-page-app projects. Uses Express, MongoDB, Backbone, Browserify, LESS and Gulp.",
	"repository": "https://github.com/spencerbeggs/spa-skeleton",
	"main": "index.js",
	"author": "C. Spencer Beggs <spencer@beg.gs>",
	"license": "Private",
	"scripts": {
		"start": "node index.js",
		"proxy": "sudo pfctl -evf hooks/pd.conf",
		"config": "rm -rf ./node_modules/config;npm install config",
		"dev": "npm run config && NODE_ENV=development ./node_modules/gulp/bin/gulp.js dev",
		"build": "gulp build"
	},
	"dependencies": {
		"body-parser": "^1.10.1",
		"browser-sync": "2.2.3",
		"browserify": "^8.1.1",
		"browserify-shim": "3.8.2",
		"compression": "^1.3.0",
		"config": "file:./config",
		"cookie-parser": "^1.3.3",
		"drag-drop": "^2.0.0",
		"dynamic-reverse-proxy": "^0.6.0",
		"envify": "^3.3.0",
		"express": "^4.11.0",
		"express-device": "^0.3.11",
		"express-handlebars": "^1.1.0",
		"gulp": "3.8.10",
		"gulp-beautify": "^1.1.0",
		"gulp-concat": "^2.4.1",
		"gulp-eol": "^0.1.1",
		"gulp-flatten": "0.0.4",
		"gulp-jsbeautifier": "0.0.4",
		"gulp-jshint": "^1.8.6",
		"gulp-json-format": "^1.0.0",
		"gulp-less": "^2.0.0",
		"gulp-minify-css": "^0.3.11",
		"gulp-nodemon": "^1.0.4",
		"gulp-notify": "^2.0.0",
		"gulp-pleeease": "^1.1.0",
		"gulp-plumber": "^0.6.6",
		"gulp-sourcemaps": "^1.2.8",
		"gulp-strip-debug": "^1.0.1",
		"gulp-uglify": "^1.0.1",
		"gulp-util": "^3.0.1",
		"hbsfy": "^2.2.0",
		"html-minifier": "^0.7.0",
		"jquery": "^2.1.3",
		"less-plugin-autoprefix": "^1.0.1",
		"lodash": "^2.4.1",
		"nib": "^1.0.4",
		"pretty-hrtime": "^1.0.0",
		"run-sequence": "^1.0.1",
		"serve-favicon": "^2.2.0",
		"supervisor": "~0.6.0",
		"vinyl-buffer": "^1.0.0",
		"vinyl-source-stream": "^1.0.0",
		"watchify": "^2.2.1"
	},
	"browser": {
		"config": "./config/index.js"
	},
	"engines": {
		"node": "0.12.x"
	},
	"browserify": {
		"transform": ["envify"]
	}
}
